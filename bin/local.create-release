#!/usr/bin/env bash
set -o errexit -o errtrace -o noclobber -o nounset -o pipefail

export plugin_name=APIData
export plugin_repo_url=https://github.com/ITK-Leantime/data-api/

buildRelease() {
  composer install --no-dev --classmap-authoritative
  npm install --no-dev
  npm run build
  return
}

export -f buildRelease
